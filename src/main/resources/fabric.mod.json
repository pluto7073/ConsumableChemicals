{
  "schemaVersion": 1,
  "id": "chemicals",
  "version": "${version}",
  "name": "Consumable Chemicals",
  "description": "A library mod that provides functionality for chemicals that can be consumed by players",
  "authors": [
    "Pluto7073"
  ],
  "contact": {
    "sources": "https://github.com/pluto7073/ConsumableChemicals/"
  },
  "license": "MIT",
  "icon": "assets/chemicals/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "ml.pluto7073.chemicals.Chemicals"
    ]
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_4174": [ "ml/pluto7073/chemicals/item/internal/ChemicalHolder" ],
      "net/minecraft/class_4174$class_4175": [ "ml/pluto7073/chemicals/item/internal/ChemicalHolderBuilder" ]
    }
  },
  "accessWidener": "chemicals.accesswidener",
  "mixins": [
    "chemicals.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": ">=${fabric_version}",
    "minecraft": "${minecraft_dependency}"
  }
}
